{
  "tags": {
    "allowUnknownTags": ["component"]
},
"plugins": [
    "node_modules/better-docs/typescript",
    "node_modules/better-docs/component"
],
"source": {
  "include": ["src"],
  "includePattern": ".+\\.ts(doc|x)?$",
  "excludePattern": "(^|\\/|\\\\)_"
},
    "typescript": {
      "moduleRoot": "test"
  },
    "templates": {
      "better-docs": {
        "name": "My React components"
      }
    },
    "opts": {
      "destination": "docs",
      "recurse": true,
      "readme": "README.md"
    }
}